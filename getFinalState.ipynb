      "source": [
        "class Solution(object):\n",
        "    def getFinalState(self, nums, k, multiplier):\n",
        "\n",
        "        for _ in range(k):\n",
        "\n",
        "            min_index = nums.index(min(nums))\n",
        "\n",
        "            nums[min_index] *= multiplier\n",
        "        return nums\n"
      ]
    }
  ]
