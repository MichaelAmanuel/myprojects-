      "source": [
        "class Solution(object):\n",
        "    def createTargetArray(self, nums, index):\n",
        "\n",
        "      o_p = []\n",
        "      for i,j in zip(nums, index):\n",
        "        o_p = o_p[:j] + [i] + o_p[j:]\n",
        "\n",
        "      return o_p\n",
        "\n",
        "solution = Solution()\n",
        "nums = [0,1,2,3,4]\n",
        "index = [0,1,2,2,1]\n",
        "print(solution.createTargetArray(nums, index))"
      ]
