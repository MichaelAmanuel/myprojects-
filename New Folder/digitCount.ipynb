        "\n",
        "class Solution(object):\n",
        "    def digitCount(self, num):\n",
        "      for i, j in zip(range(len(num)),num):\n",
        "        if num.count(str(i)) != int(j):\n",
        "\n",
        "          return False\n",
        "\n",
        "      return True\n",
        "\n",
        "solution = Solution()\n",
        "num = \"030\"\n",
        "print(solution.digitCount(num))"
      ]
    }
