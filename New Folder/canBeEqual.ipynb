        "class Solution:\n",
        "    def canBeEqual(self, target, arr):\n",
        "        return sorted(target) == sorted(arr)\n",
        "\n",
        "solution = Solution()\n",
        "target = [1,2,3,4]\n",
        "arr = [2,4,1,3]\n",
        "print(solution.canBeEqual(target, arr))"
      ]
