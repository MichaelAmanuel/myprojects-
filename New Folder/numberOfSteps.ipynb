        "\n",
        "class Solution(object):\n",
        "    def numberOfSteps(self, n):\n",
        "\n",
        "      count = 0\n",
        "      while n > 0:\n",
        "        if n % 2 == 0:\n",
        "          n = n // 2\n",
        "        else:\n",
        "          n = n- 1\n",
        "\n",
        "        count += 1\n",
        "\n",
        "      return count\n",
        "\n",
        "solution = Solution()\n",
        "n = 123\n",
        "print(solution.numberOfSteps(n))"
      ]
    }
