      "source": [
        "class Solution(object):\n",
        "    def maxProduct(self, nums):\n",
        "      ma = sorted(nums, reverse=True)\n",
        "      result = (ma[0] - 1) * (ma[1]-1)\n",
        "      return result\n",
        "\n",
        "solution = Solution()\n",
        "nums = [3,4,5,2]\n",
        "print(solution.maxProduct(nums))"
      ]
    }
  ]
}