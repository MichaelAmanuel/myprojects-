      "source": [
        "class Solution(object):\n",
        "    def shuffle(self, nums, n):\n",
        "\n",
        "      o_p = []\n",
        "\n",
        "      for i, j in zip(nums[:n], nums[n:]):\n",
        "        o_p.append(i)\n",
        "        o_p.append(j)\n",
        "\n",
        "      return o_p\n",
        "\n",
        "solution = Solution()\n",
        "nums = [1,2,3,4,4,3,2,1]\n",
        "n = 4\n",
        "print(solution.shuffle(nums, n))"
      ]
