      "source": [
        "class Solution(object):\n",
        "    def sumOfMultiples(self, n):\n",
        "      su = 0\n",
        "      for i in range(1, n+1):\n",
        "        if i % 3 == 0 or i % 5 == 0 or i % 7 == 0:\n",
        "          su += i\n",
        "\n",
        "      return su\n",
        "\n",
        "solution = Solution()\n",
        "n = 10\n",
        "print(solution.sumOfMultiples(n))"
