      "source": [
        "class Solution:\n",
        "    def passThePillow(self, n, time):\n",
        "        time = time % (2 * (n - 1))\n",
        "        if time < n:\n",
        "            return time + 1\n",
        "        else:\n",
        "            return 2 * n - time - 1\n",
        "\n",
        "# Example usage:\n",
        "solution = Solution()\n",
        "print(solution.passThePillow(4, 5))"
      ]
    }
  ]
