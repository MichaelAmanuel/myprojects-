        "vowels = ['a', 'e', 'i', 'o', 'u']\n",
        "\n",
        "s = \"hello\"\n",
        "ss = ''\n",
        "for i in range(len(s)):\n",
        "    if s[i] in vowels:\n",
        "        for i in range(len(s)-1, -1, -1):\n",
        "            if s[i] in vowels:\n",
        "                ss += s[i]\n",
        "            break\n",
        "\n",
        "    else:\n",
        "        ss += s[i]\n",
        "\n",
        "print(ss)\n"
      ]
    }
