      "source": [
        "class Solution(object):\n",
        "    def maxProductDifference(self, nums):\n",
        "\n",
        "      num = sorted(nums)\n",
        "      result = ( num[len(num)-1] * num[len(num)-2]) - (num[0]*num[1])\n",
        "\n",
        "      return result\n",
        "\n",
        "solution = Solution()\n",
        "nums = [5,6,2,7,4]\n",
        "print(solution.maxProductDifference(nums))"
      ]
    }
  ]
}