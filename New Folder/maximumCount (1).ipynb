      "source": [
        "class Solution:\n",
        "    def maximumCount(self, nums):\n",
        "        neg_count = sum(1 for x in nums if x < 0)\n",
        "        pos_count = sum(1 for x in nums if x > 0)\n",
        "        return max(neg_count, pos_count)\n"
      ]
