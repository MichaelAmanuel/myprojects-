        "class Solution:\n",
        "    def doesValidArrayExist(self, derived):\n",
        "        n = len(derived)\n",
        "        xor_sum = 0\n",
        "\n",
        "        for num in derived:\n",
        "            xor_sum ^= num\n",
        "\n",
        "        return xor_sum == 0"
      ]
    }
  ]
