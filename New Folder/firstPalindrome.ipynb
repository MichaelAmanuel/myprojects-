      "source": [
        "class Solution(object):\n",
        "  def firstPalindrome(self, words):\n",
        "\n",
        "    for i in words:\n",
        "      if i == i[::-1]:\n",
        "        return i\n",
        "    return ''\n",
        "\n",
        "solution = Solution()\n",
        "words = [\"def\",\"ghi\"]\n",
        "print(solution.firstPalindrome(words))"
      ]
    }
  ]
