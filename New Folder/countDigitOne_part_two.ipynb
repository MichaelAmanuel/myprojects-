      "source": [
        "class Solution(object):\n",
        "    def countDigitOne(self, n):\n",
        "\n",
        "      count = 0\n",
        "      for i in range(n + 1):\n",
        "        x = str(i).count('1')\n",
        "        count += x\n",
        "\n",
        "      return count\n",
        "\n",
        "solution = Solution()\n",
        "n = 824883294\n",
        "print(solution.countDigitOne(n))"
      ]
