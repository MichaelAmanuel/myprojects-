        "class Solution(object):\n",
        "    def sumOfSquares(self, nums):\n",
        "\n",
        "      su = 0\n",
        "\n",
        "      for i in range(1, len(nums)+1):\n",
        "        if len(nums) % i == 0:\n",
        "          su += (nums[i - 1]**2)\n",
        "\n",
        "      return su\n",
        "\n",
        "solution = Solution()\n",
        "nums = [2,7,1,19,18,3]\n",
        "print(solution.sumOfSquares(nums))"
      ]
    }
  ]
