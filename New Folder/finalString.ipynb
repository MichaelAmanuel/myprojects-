      "source": [
        "\n",
        "class Solution(object):\n",
        "    def finalString(self, s):\n",
        "\n",
        "      o = ''\n",
        "      for i in s:\n",
        "        if i != 'i':\n",
        "          o += i\n",
        "        else:\n",
        "          o = o[::-1]\n",
        "\n",
        "      return o\n",
        "\n",
        "solution = Solution()\n",
        "s = \"string\"\n",
        "print(solution.finalString(s))"
      ]
