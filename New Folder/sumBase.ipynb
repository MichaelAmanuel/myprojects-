        "class Solution(object):\n",
        "    def sumBase(self, n, k):\n",
        "      dig = 0\n",
        "      while n > 0:\n",
        "        dig = dig + n % k\n",
        "        n = n // k\n",
        "\n",
        "      return dig\n",
        "\n",
        "solution = Solution()\n",
        "n = 10\n",
        "k = 10\n",
        "print(solution.sumBase(n, k))"
      ]
    }
  ]
