      "source": [
        "class Solution(object):\n",
        "    def addStrings(self, num1, num2):\n",
        "        sum_str = int(num1) + int(num2)\n",
        "        return str(sum_str)\n",
        "\n",
        "solution = Solution()\n",
        "num1 = \"11\"\n",
        "num2 = \"123\"\n",
        "print(solution.addStrings(num1, num2))"
      ]
    }
