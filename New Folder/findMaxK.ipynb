      "source": [
        "class Solution(object):\n",
        "    def findMaxK(self, nums):\n",
        "\n",
        "      num = sorted(nums, reverse=True)\n",
        "      for i in num:\n",
        "        if (-1 * i) in nums:\n",
        "          return i\n",
        "      return -1\n",
        "\n",
        "solution = Solution()\n",
        "nums = [-1,2,-3,3]\n",
        "print(solution.findMaxK(nums))"
      ]
    }
  ]
