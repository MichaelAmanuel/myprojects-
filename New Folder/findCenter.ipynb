        "\n",
        "class Solution(object):\n",
        "    def findCenter(self, edges):\n",
        "        return edges[0][0] if edges[0][0] in edges[1] else edges[0][1]\n",
        "\n",
        "solution = Solution()\n",
        "edges = [[1, 2], [2, 3], [4, 2]]\n",
        "print(solution.findCenter(edges))"
      ]
    }
