      "source": [
        "class Solution(object):\n",
        "    def countBits(self, n):\n",
        "      out_put = []\n",
        "      for i in range(n+1):\n",
        "        answer = (bin(i)[2:])\n",
        "        out_put.append(int(answer.count('1')))\n",
        "\n",
        "      return out_put\n",
        "\n",
        "solution = Solution()\n",
        "n = 5\n",
        "print(solution.countBits(n))"
      ]
    }
  ]
}