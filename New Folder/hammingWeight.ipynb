        "class Solution(object):\n",
        "    def hammingWeight(self, n):\n",
        "\n",
        "        return bin(n).count('1')\n",
        "\n",
        "\n",
        "solution = Solution()\n",
        "print(solution.hammingWeight(11))\n",
        "print(solution.hammingWeight(128))\n",
        "print(solution.hammingWeight(255))\n",
