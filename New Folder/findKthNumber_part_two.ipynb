      "source": [
        "class Solution(object):\n",
        "    def findKthNumber(self, n, k):\n",
        "\n",
        "      li = []\n",
        "      for i in range(1, n+1):\n",
        "        li.append(str(i))\n",
        "\n",
        "      re = sorted(li)\n",
        "\n",
        "      return int(re[k-1])\n",
        "\n",
        "solution = Solution()\n",
        "n = 100\n",
        "k = 2\n",
        "print(solution.findKthNumber(n , k))"
