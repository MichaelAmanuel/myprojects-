      "source": [
        "from collections import Counter\n",
        "\n",
        "class Solution:\n",
        "    def uncommonFromSentences(self, s1, s2):\n",
        "        word_count = Counter(s1.split()) + Counter(s2.split())\n",
        "        return [word for word in word_count if word_count[word] == 1]\n"
      ]
    }
  ]
}