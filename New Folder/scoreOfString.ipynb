      "source": [
        "class Solution(object):\n",
        "    def scoreOfString(self, s):\n",
        "        score = 0\n",
        "        for i in range(1, len(s)):\n",
        "            score += abs(ord(s[i]) - ord(s[i - 1]))\n",
        "        return score\n",
        "\n",
        "\n",
        "solution = Solution()\n",
        "s = \"hello\"\n",
        "result = solution.scoreOfString(s)\n",
        "print(result)"
      ]
    }
  ]
