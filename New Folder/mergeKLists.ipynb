      "source": [
        "\n",
        "class Solution(object):\n",
        "    def mergeKLists(self, lists):\n",
        "      o_p = []\n",
        "      for i in lists:\n",
        "        for j in i:\n",
        "          o_p.append(j)\n",
        "\n",
        "      return sorted(o_p)\n",
        "\n",
        "solution = Solution()\n",
        "lists = [[1,4,5],[1,3,4],[2,6]]\n",
        "print(solution.mergeKLists(lists))"
      ]
    }
  ]
