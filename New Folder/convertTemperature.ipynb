      "source": [
        "class Solution(object):\n",
        "    def convertTemperature(self, celsius):\n",
        "      out_p = [(celsius + 273.15), (celsius * 1.80 + 32.00)]\n",
        "      return out_p\n",
        "\n",
        "solution = Solution()\n",
        "celsius = 36.50\n",
        "print(solution.convertTemperature(celsius))"
      ]
    }
  ]
