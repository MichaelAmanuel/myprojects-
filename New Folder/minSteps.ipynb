        "class Solution:\n",
        "    def minSteps(self, n):\n",
        "        steps = 0\n",
        "        divisor = 2\n",
        "\n",
        "        while n > 1:\n",
        "            while n % divisor == 0:\n",
        "                steps += divisor\n",
        "                n //= divisor\n",
        "            divisor += 1\n",
        "\n",
        "        return steps\n"
      ]
    }
  ]
