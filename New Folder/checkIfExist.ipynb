      "source": [
        "class Solution(object):\n",
        "    def checkIfExist(self, arr):\n",
        "\n",
        "        seen = set()\n",
        "        for num in arr:\n",
        "            if num * 2 in seen or (num % 2 == 0 and num // 2 in seen):\n",
        "                return True\n",
        "            seen.add(num)\n",
        "        return False"
      ]
    }
  ]
}